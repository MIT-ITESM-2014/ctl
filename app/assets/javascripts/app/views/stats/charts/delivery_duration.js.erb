(function(){
  $(function(){
    var el = app.stats.charts.delivery_duration = {};
    $.extend(el,{
      parent: app.stats.charts,
      init: function(){
        var base = this.parent.getChart('delivery_duration');
        $.extend(base, this);
        $.extend(this, base);
        this.initSlide();
      },
      setLang: function(){
        var local = this.lang.chart;
        $.extend(this.opts, {
          title: {
            text: local.title
          },
          subtitle: {
            text: local.subtitle
          },
          credits: {
            text: local.credits
          },
          yAxis: {
            //min: -20,
            title: {
              text: local.y_axis
            }
          },
          xAxis: {
            categories: this.getXAxis(),
            lineWidth: 0,
            title: {
              text: local.x_axis
            }
          }
        });
      },
      setData: function(){
        console.log(this.data);
        $.extend(this.opts, {
          series: [{
            name: 'Delivery Duration',
            data: this.data.data,
            marker: {
	            fillColor: 'white',
	            lineWidth: 1,
	            lineColor: Highcharts.getOptions().colors[0]
	          }
          }]
        });
      }
    });
    el.init();
  });
})(jQuery);
