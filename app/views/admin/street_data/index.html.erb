<% content_for :table_fields do %>
	<th data-sort="blocks.research_id" data-name="b_research"><%= t 'app.admin.views.street_data.fields.block_research' %></th>
	<th data-sort="streets.research_id" data-name="s_research"><%= t 'app.admin.views.street_data.fields.street_research' %></th>
	<th data-sort="street_data.created_at" data-name="c_at"><%= t 'app.admin.views.street_data.fields.created_at' %></th>
<% end %>
<% action_url = @section[:params].merge(action: :action) %>
<%= javascript_include_tag url_for(action_url) %>
<%= render partial: 'admin/shared/section/section', locals: { opts: @section.merge(show_add_btn: edit?) } %>