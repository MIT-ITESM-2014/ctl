<% content_for :table_formats do %>
var table_formats = {};
$.extend(table_formats, {
	parent: this,
	root: null,
	list: {},
	init: function(){
		this.initRoot();
		this.initList();
	},
	initList: function(){
		var _self = this;
		$.extend(this.list, {
			c_at: "date"
		});
	},
	initRoot: function(){
		this.root = this.parent.root;
	}
});
table_formats.init();
this.table_formats = table_formats.list;
<% end %>
