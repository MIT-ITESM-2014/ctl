<% content_for :head do %>
	<%= stylesheet_link_tag 'app/views/about' %>
	<%= javascript_include_tag 'app/views/about' %>
<% end %>
<div id="about">
	<div class="panel view">
		<div class="panel_wrapper">
			<div class="panel_header">
				<div class="panel_title_wrapper">
					<h1 class="panel_title"><%= t 'app.views.about.map.title' %></h1>
					<div class="clear"></div>
				</div>
				<p class="panel_subtitle"><%= t 'app.views.about.map.legend' %></p>
			</div>
			<div class="panel_content">
				
			</div>
		</div>
	</div>
</div>